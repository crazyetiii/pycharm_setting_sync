<templateSet group="methodTemplates">
  <template name="*" value="/**&#10; * @title $title$&#10; * @description $description$&#10; * @author ocean $param$&#10; * @updateTime $date$ $TIME$&#10; */" description="add method description" toReformat="false" toShortenFQNames="true">
    <variable name="title" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="description" expression="" defaultValue="" alwaysStopAt="false" />
    <variable name="param" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>